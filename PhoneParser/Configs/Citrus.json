{
	"CheckXpaths": {
		"DetailLinkSelector": "//a[@class='card-product-link']",
		"PhoneNameSelector": "//a[@class='card-product-link']//text()"
	},
	"UrlResolveRules": {
		"DetailLinkTemplate": "https://www.citrus.ua/{0}",
		"PageLinkTemplate": "https://www.citrus.ua/smartfony/page_{0}/"
	},
	"SingleXPathSelectors": {
		"ScreenDiag": "//span[contains(.,'Размер экрана')]/following::span[1]",
		"ScreenResolution": "//span[contains(.,'Разрешение экрана PX')]/following::span[1]",
		"MainCamera": "//span[contains(.,'Основная камера, Мп')]/following::span[1]",
		"ProcessorName": "//span[contains(.,'Процессор')]/following::span[1]",
		"ProcessorCores": "//span[contains(.,'Количество ядер')]/following::span[1]",
		"ProcessorSpeed": "//span[contains(.,'Частота процессора')]/following::span[1]",
		"VideoProcName": "//span[contains(.,'Видеопроцессор')]/following::span[1]",
		"RAM": "//span[contains(.,'Объем оперативной памяти')]/following::span[1]",
		"InternalMem": "//span[contains(.,'Объем встроенной памяти')]/following::span[1]",
		"IsMemExpanded": "//span[contains(.,'Слот для карты памяти')]/following::span[1]",
		"MaxInternalMem": "//span[contains(.,'Максимальный объем карты памяти')]/following::span[1]",
		"OS": "//span[contains(.,'Операционная система')]/following::span[1]",
		"AccessTypes": "//span[contains(.,'Тип связи')]/following::span[1]",
		"SIMQuantity": "//span[contains(.,'Количество SIM-карт')]/following::span[1]",
		"NavAbility": "//span[contains(.,'Навигационные возможности')]/following::span[1]",
		"BatteryCapacity": "//span[contains(.,'Емкость аккумулятора')]/following::span[1]",
		"StandaloneSpecs": "//span[contains(.,'Автономная работа')]/following::span[1]",
		"WirelessCharge": "//span[contains(.,'Функция беспроводной зарядки')]/following::span[1]",
		"FastCharge": "//span[contains(.,'Функция быстрой зарядки')]/following::span[1]",
		"BodyMaterial": "//span[contains(.,'Материал корпуса')]/following::span[1]",
		"WaterProtection": "//span[contains(.,'Водозащита')]/following::span[1]",
		"Size": "//span[contains(.,'Габариты (ВхШхГ)')]/following::span[1]",
		"Weight": "//span[contains(.,'Вес')]/following::span[1]",
		"PhoneName": "//h1[@class='cart-main-title']/following::span[1]",
		"FrontalCamera": "//span[contains(.,'Фронтальная камера, Мп')]/following::span[1]",
		"Price": "//span[@class='price-number']",
		"InStock": "//span[contains(., 'Есть в наличии')] | //span[contains(., 'Нет в наличии')] | //span[contains(., 'Количество ограничено')]"
	}
}